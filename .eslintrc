{    "env": {        "es6": true,        "browser": true    },    "extends": ["airbnb-base", "plugin:react/recommended", "plugin:react-hooks/recommended"],    "settings": {        "react": {            "createClass": "createReactClass",            "pragma": "React",            "version": "detect"        }    },    "parser": "babel-eslint",    "parserOptions": {        "sourceType": "module",        "ecmaFeatures": {            "jsx": true        }    },    "plugins": ["prettier", "react", "react-hooks"],    "rules": {        "react-hooks/rules-of-hooks": "error",        "react-hooks/exhaustive-deps": "warn",        "linebreak-style": ["error", "windows"],        "operator-linebreak": ["error", "after"],        "indent": [            "error",            4,            {                "SwitchCase": 1,                "VariableDeclarator": 1,                "outerIIFEBody": 1,                "FunctionDeclaration": {                    "parameters": 1,                    "body": 1                },                "FunctionExpression": {                    "parameters": 1,                    "body": 1                }            }        ],        "object-curly-newline": "off",        "comma-dangle": "off",        "no-plusplus": "off",        "quotes": ["error", "single"],        "arrow-parens": ["error", "always"],        "vars-on-top": ["off"],        "prefer-destructuring": "off",        "no-use-before-define": "off",        "no-confusing-arrow": "off",        "class-methods-use-this": "off",        "func-names": ["off"],        "prefer-template": ["off"],        "no-underscore-dangle": ["off"],        "no-param-reassign": ["off"],        "global-require": ["off"],        "import/extensions": "off",        "import/first": "off",        "import/no-unresolved": "off",        "import/no-extraneous-dependencies": "off",        "import/prefer-default-export": "off",        "implicit-arrow-linebreak": "off",        "no-void": ["warn"],        "max-len": ["warn"],        "no-mixed-operators": ["warn"],        "prettier/prettier": [            "error",            {                "endOfLine": "crlf"            }        ]    },    "overrides": [        {            "files": ["**/*.ts", "**/*.tsx"],            "env": {                "browser": true,                "es6": true            },            "extends": [                "airbnb-base",                "plugin:react/recommended",                "plugin:@typescript-eslint/recommended",                "plugin:prettier/recommended"            ],            "parser": "@typescript-eslint/parser",            "plugins": ["react", "@typescript-eslint"],            "rules": {                "linebreak-style": ["error", "windows"],                "operator-linebreak": ["error", "after"],                "indent": [                    "error",                    4,                    {                        "SwitchCase": 1,                        "VariableDeclarator": 1,                        "outerIIFEBody": 1,                        "FunctionDeclaration": {                            "parameters": 1,                            "body": 1                        },                        "FunctionExpression": {                            "parameters": 1,                            "body": 1                        }                    }                ],                "comma-dangle": "off",                "no-plusplus": "off",                "quotes": ["error", "single"],                "vars-on-top": ["off"],                "prefer-destructuring": "off",                "no-use-before-define": "off",                "class-methods-use-this": "off",                "func-names": ["off"],                "prefer-template": ["off"],                "no-underscore-dangle": ["off"],                "no-param-reassign": ["off"],                "global-require": ["off"],                "import/extensions": "off",                "import/first": "off",                "import/no-unresolved": "off",                "import/no-extraneous-dependencies": "off",                "import/prefer-default-export": "off",                "implicit-arrow-linebreak": "off",                "@typescript-eslint/no-explicit-any": "off",                "no-void": ["warn"],                "max-len": ["warn"],                "no-mixed-operators": ["warn"]            },            "settings": {                "react": {                    "version": "detect"                }            }        }    ],    "globals": {        "google": true,        "Expo": true,        "Linear": true,        "ga": true,        "YT": true    }}